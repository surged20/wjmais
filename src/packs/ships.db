peFormatted = damageType.charAt(0).toUpperCase() + damageType.slice(1);\n\nconst parsedDamage = Roll.parse(damage.versatile, {})\nlet diceNum = parsedDamage[0].number;\nconst diceSize = parsedDamage[0].faces;\nif (args[0].isCritical)\n  diceNum *= 2;\n\nlet damageBacklash = await new Roll(`${diceNum}d${diceSize}`).evaluate({async: true});\n\n\nnew MidiQOL.DamageOnlyWorkflow(args[0].actor, rammingShipToken, damageBacklash.total, damageType, [rammingShipToken], damageBacklash, {flavor: `${name} - Backlash Damage (${damageTypeFormatted})`})","author":"9GlgqX11CNwmlmKz","_id":null,"img":"icons/svg/dice-target.svg","folder":null,"sort":0,"ownership":{"default":0},"flags":{},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}},"wjmais":{"location":"forward","facing":90}},"system":{"description":{"value":"\n\t\t<p>This large ram is affixed to the front of a ship is built to absorb the backlash from colliding with another ship. While this ram is equipped, when the Helmsman takes the @Compendium[wjmais.features.Collide] bridge crew action and hits, the damage dice this ship takes is reduced to d4s.</p>","chat":"","unidentified":""},"source":"WJMAiS pg. 40","quantity":1,"weight":0,"price":null,"attunement":0,"equipped":false,"rarity":"","identified":false,"activation":{"type":"action","cost":1,"condition":""},"duration":{"value":0,"units":""},"target":{"value":0,"width":null,"units":"","type":""},"range":{"value":500,"long":0,"units":"ft"},"uses":{"value":0,"max":"0","per":""},"consume":{"type":"","target":"","amount":null},"ability":"","actionType":"msak","attackBonus":"0","chatFlavor":"","critical":{"threshold":null,"damage":""},"damage":{"parts":[["3d10","bludgeoning"]],"versatile":"3d4"},"formula":"","save":{"ability":"","dc":0,"scaling":"spell"},"armor":{"value":10},"hp":{"value":10,"max":10,"dt":null,"conditions":""},"weaponType":"simpleM","baseItem":"","properties":{"fmm":true,"fxd":true,"hlm"