{
  "id": "wjmais",
  "title": "Wildjammer: More Adventures in Space",
  "description": "Wildjammer content and dnd5e extensions",
  "version": "0.5.0-beta1",
  "authors": [
    {
      "name": "Surge",
      "discord": "Surge#5715",
      "email": "surged20@gmail.com"
    }
  ],
  "compatibility": {
    "minimum": "10",
    "verified": "10.274"
  },
  "esmodules": ["scripts/wjmais.js"],
  "styles": ["styles/wjmais.css"],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/v10-dev/system.json",
        "compatibility": {
          "minimum": "2.0.0-alpha1",
          "verified": "2.0.0-alpha1"
        }
      }
    ],
    "requires": [
      {
        "id": "lib-wrapper",
        "type": "module",
        "manifest": "https://github.com/ruipin/fvtt-lib-wrapper/releases/download/v1.12.7.0/module.json",
        "compatibility": {
          "minimum": "1.12.7.0",
          "verified": "1.12.7.0"
        }
      }
    ]
  },
  "packs": [
    {
      "name": "features",
      "label": "Features",
      "systems": ["dnd5e"],
      "path": "./packs/features.db",
      "type": "Item"
    },
    {
      "name": "items",
      "label": "Items",
      "systems": ["dnd5e"],
      "path": "./packs/items.db",
      "type": "Item"
    },
    {
      "name": "quickref",
      "label": "Quick Reference",
      "systems": ["dnd5e"],
      "path": "./packs/quickref.db",
      "type": "JournalEntry"
    },
    {
      "name": "ships",
      "label": "Ships",
      "systems": ["dnd5e"],
      "path": "./packs/ships.db",
      "type": "Actor"
    },
    {
      "name": "spells",
      "label": "Spells",
      "systems": ["dnd5e"],
      "path": "./packs/spells.db",
      "type": "Item"
    }
  ],
  "url": "https://github.com/surged20/wjmais",
  "manifest": "https://github.com/surged20/wjmais/releases/latest/download/module.json",
  "download": "https://github.com/surged20/wjmais/releases/download/0.5.0-beta1/module.zip",
  "readme": "https://github.com/surged20/wjmais/blob/master/README.md",
  "license": "https://github.com/surged20/wjmais/blob/master/LICENSE",
  "bugs": "https://github.com/surged20/wjmais/issues"
}
